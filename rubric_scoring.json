      "description": "Count 4 elements and scale to 0-3",
      "formula": "(elements_found / 4) * 3",
      "max_score": 3.0,
      "expected_elements": 4,
      "conversion_table": {
        "0": 0.00,
        "1": 0.75,
        "2": 1.50,
